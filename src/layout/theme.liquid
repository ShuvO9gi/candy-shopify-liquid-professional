<!doctype html>
<html
  class="no-js"
  lang="{{ request.locale.iso_code }}"
  dir="{{ settings.text_direction }}">
  <head>
    <script async src="https://www.googleoptimize.com/optimize.js?id=OPT-W7TDXZ5"></script>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <meta name="theme-color" content="{{ settings.color_button }}">
    <link rel="canonical" href="{{ canonical_url }}">
    <link rel="preconnect" href="https://cdn.shopify.com">
    <link rel="preconnect" href="https://fonts.shopifycdn.com">
    <link rel="dns-prefetch" href="https://productreviews.shopifycdn.com">
    <link rel="dns-prefetch" href="https://ajax.googleapis.com">
    <link rel="dns-prefetch" href="https://maps.googleapis.com">
    <link rel="dns-prefetch" href="https://maps.gstatic.com">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.4/css/all.min.css">

    {%- if settings.favicon != blank -%}
      <link
        rel="shortcut icon"
        href="{{ settings.favicon | img_url: '32x32' }}"
        type="image/png" />
    {%- endif -%}

    {%- render 'seo-title' -%}

    {%- if page_description -%}
      <meta name="description" content="{{ page_description | escape }}">
  {%- endif -%}

    {%- comment -%} Cookie information {%- endcomment -%}
    {%- render 'cookie-information-scripts' -%}

    {%- render 'social-meta-tags' -%}

  {%- render 'font-face' -%}

    {% comment %} 
            {{ 'theme.css' | asset_url | stylesheet_tag }} 
        {% endcomment %}

    {%- render 'css-variables' -%}

    {%- render 'google-scripts' -%}


    <script>
          document.documentElement.className = document.documentElement.className.replace('no-js', 'js');
      
          window.theme = window.theme || {};
          theme.routes = {
      home: "{{ routes.root_url }}",
      cart: "{{ routes.cart_url | append: '.js' }}",
      cartPage: "{{ routes.cart_url }}",
      cartAdd: "{{ routes.cart_add_url | append: '.js' }}",
      cartChange: "{{ routes.cart_change_url | append: '.js' }}"
          };
          theme.strings = {
      soldOut: {{ 'products.product.sold_out' | t | json }},
      unavailable: {{ 'products.product.unavailable' | t | json }},
      stockLabel: {{ 'products.product.stock_label' | t: count: '[count]' | json }},
      willNotShipUntil: {{ 'products.product.will_not_ship_until' | t: date: '[date]' | json }},
      willBeInStockAfter: {{ 'products.product.will_be_in_stock_after' | t: date: '[date]' | json }},
      waitingForStock: {{ 'products.product.waiting_for_stock' | t | json }},
      savePrice: {{ 'products.general.save_html' | t: saved_amount: '[saved_amount]' | json }},
      cartEmpty: {{ 'cart.general.empty' | t | json }},
      cartTermsConfirmation: {{ 'cart.general.terms_confirm' | t | json }},
      searchCollections: {{ 'general.search.collections' | t | json }},
      searchPages: {{ 'general.search.pages' | t | json }},
      searchArticles: {{ 'general.search.articles' | t | json }},
      seeProduct: {{ 'products.general.see_product' | t | json }},
      blandSelv: {
        bagEmpty: {{ 'bland_selv.general.bag_empty' | t | json }},
        buyMore: {{ 'bland_selv.general.buy_more_html' | t | json }},
        closeBag: {{ 'bland_selv.general.close_bag' | t | json }},
        addToBag: {{ 'bland_selv.general.add_to_bag' | t | json }},
        from: {{ 'bland_selv.general.from' | t | json }},
        clickOn: {{ 'bland_selv.general.click_on' | t | json }},
        toAddMore: {{ 'bland_selv.general.to_add_more' | t | json }},
        addedFor: {{ 'bland_selv.general.added_for' | t | json }},
        kr: {{ 'bland_selv.general.kr' | t | json }},
        ca: {{ 'bland_selv.general.ca' | t | json }},
        ca_stk: {{ 'bland_selv.general.ca_stk' | t | json }},
        gram: {{ 'bland_selv.general.gram' | t | json }},
        stks: {{ 'bland_selv.general.stks' | t | json }},
        gift_submit: {{ 'bland_selv.general.gift_submit' | t | json }},
        rename: {
          title: {{ 'bland_selv.rename_prompt.title' | t | json}},
          description: {{ 'bland_selv.rename_prompt.description' | t | json }},
          accept: {{ 'bland_selv.rename_prompt.accept' | t | json}},
          decline: {{ 'bland_selv.rename_prompt.decline' | t | json }}
        }
      },
      header: {
        topbar: {
          hours: {{ 'header.topbar.hours' | t | json }},
          hour: {{ 'header.topbar.hour' | t | json }},
          minutes: {{ 'header.topbar.minutes' | t | json }},
          minute: {{ 'header.topbar.minute' | t | json }}
        }
      }
          };
          theme.settings = {
      dynamicVariantsEnable: true,
      dynamicVariantType: {{ settings.variant_type | json }},
      cartType: {{ settings.cart_type | json }},
      purchaseLimit: {{ settings.purchase_limit | json }},
      shippingPrice: {{ settings.shipping_price | json }},
      shippingPriceLimit: {{ settings.free_shipping_limit | json }},
      isCustomerTemplate: {% if request.page_type contains 'customers/' %}true{% else %}false{% endif %},
      moneyFormat: {{ shop.money_format | json }},
      thousandsSeperator: "{{ 100000 | money | split: '1' | last | split: '0' | first }}",
      saveType: {{ settings.product_save_type | json }},
      recentlyViewedEnabled: false,
      productImageSize: {{ settings.product_grid_image_size | json }},
      productImageCover: {{ settings.product_grid_image_fill }},
      predictiveSearch: {{ settings.predictive_search_enabled }},
      predictiveSearchType: {{ settings.search_type | json }},
      inventoryThreshold: {{ settings.inventory_threshold }},
      quickView: {{ settings.quick_shop_enable }},
      themeName: 'Impulse',
      themeVersion: "4.1.1",
      cart: {{ cart | json }}
          };
    </script>

    {{ content_for_header }}

    <script src="{{ 'vendor-scripts-v9.js' | asset_url | split: '?' | first }}" defer="defer"></script>

    {%- if shop.enabled_currencies.size > 1 -%}
      <link
        rel="preload"
        as="style"
        href="{{ 'currency-flags.css' | asset_url | split: '?' | first }}"
        onload="this.onload=null;this.rel='stylesheet'">
      <link rel="stylesheet" href="{{ 'currency-flags.css' | asset_url | split: '?' | first }}">
    {%- endif -%}
    {%- comment -%} 
            <script src="{{ 'theme.js' | asset_url }}" defer="defer"></script> 
        {%- endcomment -%}
    {%- if request.page_type contains 'customers/' -%}
      <script src="{{ 'shopify_common.js' | shopify_asset_url }}" defer="defer"></script>
    {%- endif -%}

    {%- comment -%} Grafikr scripts {%- endcomment -%}
    <!-- Swiper JS -->
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/swiper@11/swiper-bundle.min.js"></script>
    <script src="https://cdn.jsdelivr.net/jquery.slick/1.4.1/slick.min.js"></script>
    <script src="{{ 'all_sliders.js' | asset_url }}" defer></script>
    <script src="{{ 'polyfills.js' | asset_url }}" defer></script>
    <script src="{{ 'shared.js' | asset_url }}" defer></script>
    <script src="{{ 'app.js' | asset_url }}" defer></script>
    <script src="{{ 'global.js' | asset_url }}" defer></script>
    <script src="{{ 'headerScrollNew.js' | asset_url }}" defer></script>
    <script src="https://cdn.jsdelivr.net/npm/canvas-confetti@1.2.0/dist/confetti.browser.min.js" defer></script>

    {%- comment -%}<link rel="stylesheet" href="{{ 'app.css' | asset_url }}"> {%- endcomment -%}
    {% comment %} 
        {% endcomment %}
    <link rel="stylesheet" href="{{ 'output-tailwind.css' | asset_url }}">
    <link rel="stylesheet" href="{{ 'global.css' | asset_url }}">
      {%- comment -%}<link rel="stylesheet" href="{{ 'custom.css' | asset_url }}"> 
              <link rel="stylesheet" href="{{ 'custom_2.css' | asset_url }}">{%- endcomment -%}
    <link rel="stylesheet" href="{{ 'new-css-2022.css' | asset_url }}">
    <link rel="stylesheet" href="{{ 'swiper-bundle.min.css' | asset_url }}">
    {{ 'base.css' | asset_url | stylesheet_tag }}
    {%- if template.suffix == 'bland-selv-slik' -%}
      <style>
        /* Chat icon custom styling */
        @media screen and (max-width: 640px) {
          .fb_dialog_content > iframe {
            right: unset !important;
            bottom: 65px !important;
          }
        }
      </style>
    {%- endif -%}


