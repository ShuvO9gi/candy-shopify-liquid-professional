<div class="section-container backgroundColorLogos">
    <div class="page-container">
        <div class="row valignMiddle twb-mt-8 md:twb-mt-16">
            <div class="col-6 twb-mb-4">
                <div class="row valignMiddle">		
                    <div class="col-2 mobileLeft">
                        <img src="/{{ section.settings.image_1 | image_url }}">
                    </div>
                    <div class="col-2 mobileCenter">
                        <img src="/{{ section.settings.image_2 | image_url }}">
                    </div>
                    <div class="col-2 textAlignCenter mobileRight">
                        <img src="/{{ section.settings.image_3 | image_url }}">
                    </div>
                </div>
            </div>
            <div class="col-6 mobilePadding">
